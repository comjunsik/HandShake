<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="right">


        <Switch
            android:text="매칭여부 "
            android:id="@+id/accountitemitem_switch_refuse_match"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="30dp"
            android:paddingTop="10dp"
            android:gravity="right"
            />

    </LinearLayout>




    <ImageButton
        android:id="@+id/accountitem_button_randomchat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/chatstart"
        android:background="#103050"
        />


    <ImageButton
        android:text="관심사 설정"
        android:src="@drawable/intereststart"
        android:id="@+id/accountitem_button_interest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#503010"/>






</LinearLayout>